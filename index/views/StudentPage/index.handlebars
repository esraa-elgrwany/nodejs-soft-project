<body>
  
    <!-- start lainding page  -->
    <div class="landing-page">
      <div class="overley"></div>
      <section class="section1" >
        <div class="nav">
            <ul>
                <!-- <li><h1>Acadimy</h1></li> -->
                <img src="../../../images/Student_Page/logo.jpg" width="100ox" > 
                <li><a class="active" href="/students/"><i class="fa-solid fa-house-laptop"></i> Home</a></li>
                
                <li><a href="/students/contact"><i class="fa-solid fa-id-card"></i> Your Courses</a></li>
                <li><a href="/students/material"><i class="fa-solid fa-table-cells"></i> Registration</a></li>
                <li><a href="#"><i class="fa-solid fa-book-open"></i> GPA</a></li>
                <li><a href="#"> <i class="fa-solid fa-users"></i> About Us</a></li>
              </ul>
        
        </div>
        <div class="table">
            <h1>Student Information</h1>
            <table >
              <tr class="one">
                <th >Name</th>
                <th>Id</th>
                <th>Nationality</th>
                <th>Gender</th>
              </tr>
              <tr>
                {{#each students}}
                <td>{{name}}</td>
                <td>{{id}}</td>
                <td>Egyption</td>
                <td>{{gender}}</td>
                {{/each}}
              </tr>
            
            </table>
        
        
          
            <h1>Connect Information</h1>
            <table>
              <!-- <tr>
                <th class="pa">Name</th>
                <th>Id</th>
              </tr> -->
              {{#each students}}
              <tr>
                <td class="pa"> Country</td>
                <td>Cairo</td>
              
              </tr>
              <tr>
                <td class="pa"> Address</td>
                <td>{{address}}</td>
              
              </tr>
              <tr>
                <td class="pa"> phone</td>
                <td>{{phone}}</td>
          
              </tr>
              <tr>
                <td class="pa"> Qualification</td>
                <td>High school</td>
              
              </tr>
              <tr>
                <td class="pa"> Departement</td>
                <td>{{departement}}</td>
              
              </tr>
              <tr>
                <td class="pa"> Graduation Date</td>
                <td>{{year}}</td>
              
              </tr>
            {{/each}}
            </table>
            
            </div> 
        
            </section>
    </div>  
        
</body>
</html>